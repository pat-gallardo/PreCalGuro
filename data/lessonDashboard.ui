                                      </property>
                                      <property name="text">
                                       <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;Examples of solving nonlinear equations using Substitution Method.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
                                      </property>
