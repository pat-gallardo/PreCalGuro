                                  <widget class="QPushButton" name="parabolaEx1PlotButton">
                                   <property name="font">
                                    <font>
                                     <pointsize>15</pointsize>
                                    </font>
                                   </property>
                                   <property name="styleSheet">
                                    <string notr="true">#parabolaEx1PlotButton{
	color: rgb(0, 0, 0);
	border-radius: 8px;
	background-color: rgb(0, 218, 0);
	padding-top: 5px;
	padding-bottom: 5px;
